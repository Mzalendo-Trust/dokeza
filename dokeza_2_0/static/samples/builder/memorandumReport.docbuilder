function fillCell(nCurRow, nCurCol, sText, sWeight = false, sPos = "left", sColour = "black", sFontsize = 11) {
  var mcRow = mcTable.GetRow(nCurRow);
  var mcCell = mcRow.GetCell(nCurCol);
  var mcCellContent = mcCell.GetContent();
  var mcRun = mcCellContent.GetElement(0);
  var sRun = Api.CreateRun();
  sRun.SetFontFamily("Tahoma");
  sRun.AddText(sText);
  if (sWeight != false){
    sRun.SetBold(true);
  }
  if (sColour == "blue"){
    sRun.SetColor(47,84,150);
  }
  if (sFontsize != 11){
    sRun.SetFontSize(sFontsize);
  }
  mcRun.AddElement(sRun);
  if (sPos != "left"){
    mcRun.SetJc(sPos);
  }
  return {
    Cell: mcCell,
    Run: mcRun
  };
}

