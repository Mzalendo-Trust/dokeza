var nCurRow = 1;
for (var sUserName in mcCommentsReport) {
  var arrUserComments = mcCommentsReport[sUserName];
  var arrCells = [];
  for (var nIndex = 0, nCount = arrUserComments.length; nIndex < nCount; ++nIndex, ++nCurRow) {
    var mcCommentInfo = mcCommentsReport[sUserName][nIndex];
    fillCell(nCurRow, 0, (new Date(mcCommentInfo["Date"])).toString());
    fillCell(nCurRow, 1, mcCommentInfo["QuoteText"] ? mcCommentInfo["QuoteText"] : "");
    fillCell(nCurRow, 2, mcCommentInfo["CommentMessage"] ? mcCommentInfo["CommentMessage"] : "");
    fillCell(nCurRow, 3, mcCommentInfo["IsAnswer"] === true ? "reply" : "comment");
    arrCells.push(fillCell(nCurRow, 4, "").Cell);
  }
  var mcMergedCell = mcTable.MergeCells(arrCells);
  if (mcMergedCell) {
    var mcCellContent = mcMergedCell.GetContent();
    mcCellContent.GetElement(0).AddText(sUserName);
  } else if (arrCells.length > 0) {
    var mcCellContent = arrCells[0].GetContent();
    mcCellContent.GetElement(0).AddText(sUserName);
  }
}

